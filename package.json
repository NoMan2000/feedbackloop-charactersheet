{
  "name": "vuecharactersheet",
  "version": "1.0.0",
  "description": "This is a modified version of GURPS rules using Harmon Cooper's *The Feedback Loop* as the inspiration.  This means no permadeaths, resets back to the beginning of the day, etc.",
  "main": "index.js",
  "xo": {
    "extends": "./.eslintrc.js",
    "esnext": true,
    "envs": [
      "browser"
    ]
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start js_tests/unit/karma.config.js --single-run",
    "build:production:client": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --hide-modules",
    "build:dev:client": "cross-env NODE_ENV=dev webpack --config ./webpack.config.js --progress --hide-modules",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "chromedriver:start": "chromedriver --url-base=/wd/hub --port=4444",
    "htmllint": "htmllint",
    "stylelint": "stylelint",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build": "node build/build.js"
  },
